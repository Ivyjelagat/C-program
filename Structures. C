	float height ;
	int phone_no ;
	
} student1 ,student2;

int main (){
	// struct student student 1,student 2
	strcpy(student1 .name,"ivy");
	strcpy(student1 . email,"ivykemboi@gmail.com");
	
	strcpy(student1 .Reg_no,"CT101/G/24882/24");
	student1.height =5.7;
	student1.phone_no =712345678;

	
	
	printf("name:%s\n",student1. name);
	printf("email:%s\n",student1.email);
	printf("Reg_no:%s\n",student1.Reg_no);
	printf("height:%f\n",student1.height);
	printf("phone_no:%d\n",student1.phone_no);
	
	return 0;

}
